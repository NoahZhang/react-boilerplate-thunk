{
  "name": "react-boilerplate-thunk",
  "version": "0.0.1",
  "description": "",
  "main": "app/main.jsx",
  "scripts": {
    "lint": "eslint 'app/**/*.@(js|jsx)'",
    "lint:msg": "validate-commit-msg",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --colors --content-base app",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js --progress",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.5",
    "classnames": "^2.2.3",
    "exports-loader": "^0.6.3",
    "history": "^2.0.1",
    "i18next": "^2.4.1",
    "i18next-xhr-backend": "^0.5.3",
    "immutable": "^3.8.0",
    "imports-loader": "^0.6.5",
    "lodash": "^4.8.2",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-antd": "^0.4.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "commitizen": "^2.7.6",
    "conventional-changelog": "^1.1.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "ghooks": "^1.2.0",
    "html-webpack-plugin": "^2.19.0",
    "open-browser-webpack-plugin": "0.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.5.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ruan Yifeng",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "npm run lint:msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
